{
    "routes": [
        {
            "route": "/profile",
            "allowedRoles": ["admin", "contributor", "custom", "reader", "authenticated"]
        },
        {
            "route": "/dashboard",
            "allowedRoles": ["admin"]
        }
    ]
}
{
  "auth": {
    "identityProviders": {
      "github": {
        "registration": {
          "clientIdSettingName": "GITHUB_CLIENT_ID",
          "clientSecretSettingName": "GITHUB_CLIENT_SECRET"
        },
        "orgs": ["Testing authorisation"]
      }
    }
  }
}
{
  "responseOverrides": {
    "401": {
      "redirect": "/.auth/login/github?post_login_redirect_uri=.referrer",
      "statusCode": 302
    }
  }
}
{
  "route": "/login",
  "redirect": "/.auth/login/github"
}
